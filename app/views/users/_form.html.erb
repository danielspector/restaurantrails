<%= form_for @user do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :email %>
<%= f.text_field :email %>
<br>
<%= f.label :password %>
<%= f.password_field :password %>
<br>
<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>
<br>
<%= f.label :zipcode_list %>
<%= f.text_field :zipcode_list %>
<br>
<%= f.label :cuisine_list %>
<%= f.text_field :cuisine_list %>
<br>
<%= f.label :restaurant_list %>
<%= f.text_field :restaurant_list %>

<%= f.submit %>
<% end %>